{
    "name": "aligilan/laravel-recaptcha",
    "description": "Simple and painless Google reCAPTCHA package for Laravel 5.5 or greater",
    "version": "1",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "recaptcha",
        "captcha",
        "laravel",
        "validation"
    ],
    "homepage": "https://github.com/aligilan/laravel-recaptcha",
    "authors": [
        {
            "name": "Roberto Belotti",
            "email": "roby.belotti@gmail.com",
            "homepage": "https://biscolab.com",
            "role": "Developer"
        },
        {
            "name": "Aligilan",
            "email": "ali.gilan.ag@gmail.com",
            "homepage": "https://aligilan.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "6.*|7.*"
    },
    "autoload": {
        "psr-4": {
            "Aligilan\\ReCaptcha\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aligilan\\ReCaptcha\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "suggest": {

    },
    "extra": {
        "laravel": {
            "providers": [
                "Aligilan\\ReCaptcha\\ReCaptchaServiceProvider"
            ],
            "aliases": {
                "ReCaptcha": "Aligilan\\ReCaptcha\\Facades\\ReCaptcha"
            }
        }
    }
}
